{
  "name": "codeinc/utf8-sanitizer",
  "version": "1.2",
  "description": "Sanitize UTF-8 strings",
  "homepage": "https://github.com/codeinchq/utf8-sanitizer",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "ext-iconv": "*"
  },
  "autoload": {
    "psr-4": {
      "CodeInc\\Utf8Sanitizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CodeInc\\Tests\\Utf8Sanitizer\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.co",
      "homepage": "https://www.codeinc.co",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "spatie/ray": "^1.34"
  }
}
